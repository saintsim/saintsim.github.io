{
    "compilerOptions": {
        "moduleResolution": "node",  // Add this line
        "outDir": "./built",
        "allowJs": true,
        "module": "esnext",
        "target": "esnext",
        "lib": ["esnext", "ES2015", "dom", "dom.iterable"],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": ["vite/client"]
    },
    "include": ["./src/**/*"]
}